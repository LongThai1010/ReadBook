<div class="mt-4">
    <h1>All users</h1>

    <table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Username </th>
      <th scope="col">Password</th>
      <th scope="col" >Email</th>
      <th scope="col">Role (0: ADMIN, 1: USER)</th>
    </tr>
  </thead>
  <tbody>

        {{#each user}}
    <tr>
      <th scope="row">{{sum @index 1 }}</th>
      <td>{{this.username}}</td>
     
      <td>{{this.password}}</td>
      <td>{{this.email}}</td>
      <td>{{this.role}}</td>
    </tr>
    
        {{/each}}
    
  </tbody>
</table>
